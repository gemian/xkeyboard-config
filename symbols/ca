    key <AE02>	{ [   twosuperior,  VoidSymbol ]	};
    key <AE03>	{ [ threesuperior,    sterling ]	};
    key <AE04>	{ [    onequarter,    currency ]	};
    key <AE05>	{ [       onehalf,threeeighths ]	};
    key <AE06>	{ [ threequarters, fiveeighths ]	};
