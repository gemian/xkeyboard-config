
partial hidden alphanumeric_keys modifier_keys
xkb_symbols "modifiers" {
    key <LWIN> { [ISO_Level3_Shift ] };
    modifier_map Mod5 { ISO_Level3_Shift };

};

partial hidden alphanumeric_keys
xkb_symbols "common_keys" {
    key <POWR>  { [    Escape,       Escape,       XF86Sleep, XF86PowerDown ] };
    key <BKSP>  { [ BackSpace,       Delete,       XF86Tools ] };
    key <TAB>   { [       Tab, ISO_Left_Tab,       Caps_Lock ] };
    key <UP>    { [        Up,        Prior,           Prior ] };
    key <SPCE>  { [     space,        space, XF86AudioRecord,  nobreakspace ] };
    key <LEFT>  { [      Left,         Home,            Home ] };
    key <DOWN>  { [      Down,         Next,            Next ] };
    key <RGHT>  { [      Right,         End,             End  ] };
};

partial hidden alphanumeric_keys
xkb_symbols "escape" {
    key <POWR>  { [    Escape ] };
};

